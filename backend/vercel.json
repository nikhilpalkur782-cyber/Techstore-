{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "env": {
        "MONGO_URI": "@mongo_uri",
        "JWT_SECRET": "@jwt_secret",
        "NODE_ENV": "production",
        "PAYPAL_CLIENT_ID": "@paypal_client_id"
    }
}